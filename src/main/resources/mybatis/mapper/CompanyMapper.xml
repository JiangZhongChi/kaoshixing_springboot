<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kaoshixing_springboot.mapper.CompanyMapper">

    <select id="queryCompanyById" resultType="Company">
        SELECT * FROM company WHERE id = #{id}
